{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "This repo contains a basic Node and Express app to get you started in constructing an API. To get started, clone this repo and run `yarn` in your terminal at the project root.",
  "main": "server.ts",
  "scripts": {
    "start": "nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "test": "ENV=test db-migrate --env test up && jasmine-ts && npm run drop-database",
    "tsc": "tsc",
    "db-migrate": "db-migrate create orders --sql-file",
    "up": "db-migrate up",
    "down": "db-migrate down",
    "jasmine": "jasmine init",
    "drop-database": "db-migrate reset --env test"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.9",
    "@types/pg": "^7.14.7",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jasmine": "^3.7.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.6.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.10",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/request": "^2.48.5",
    "@types/supertest": "^2.0.11",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.3",
    "nodemon": "^2.0.7",
    "request": "^2.88.2",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ardeshirsaadat/storeFront_nodeJS_project.git"
